*Write time 

alias measurement TmpWrite{
	run tran (stop = 10n)
	export real CurX = abs(I(Vtest)@6n)

	export real TpWrt = 1n - TpWrite

}
search TpWrite from binary(start=0.9n, stop=0.7n, tol=1f){
	run TmpWrite
}while (TmpWrite->CurX < 0.00132 && TmpWrite->CurX > 0.00128)
